void selectionsort(int arr[],int n)
{
    for(int i=0;i<n-1;i++)
    {
        int k=i;
        for(int j=i;j<n;j++)
        {
            if(arr[j]<arr[k])
            k=j;
        }
    
    int temp=arr[k];
    arr[k]=arr[i];
    arr[i]=temp;
    }
     for(int i=0;i<n;i++)
    cout<<arr[i]<<" ";
}
